{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false, 
    "anonymousAuthentication": true, 
    "iisExpress": {
      "applicationUrl": "http://localhost:14835",
      "sslPort": 44337 //puerto en el que se abre la app
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger", //cuando lo corres abre esta url por default
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" //setea el ambiente en el que estas, el env.IsDevelopment se fija aca
      }
    },
    "WebApiObjetos": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/values",
      "applicationUrl": "http://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // setea el ambiente en el que estas, el env.IsDevelopment se fija aca o aca podria ser tambien.
      }
    }
  }
}